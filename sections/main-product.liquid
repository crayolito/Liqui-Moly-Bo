      "info": "t:sections.main_product.settings.image_disclaimer.info"
    },
    {
      "type": "checkbox",
      "id": "collapse_other_expandables",
      "default": false,
      "label": "t:sections.main_product.settings.collapse_other_expandables.label",
      "info": "t:sections.main_product.settings.collapse_other_expandables.info"
    }
  ],
  "blocks": [
    {
      "type": "@app"
    },
    {
      "type": "title",
      "name": "t:sections.main_product.blocks.title.name",
      "limit": 1,
      "settings": [
        {
          "type": "checkbox",
          "id": "show_vendor",
          "label": "t:sections.main_product.blocks.title.settings.show_vendor.label",
          "default": true
        },
        {
          "type": "checkbox",
          "id": "show_stock",
          "label": "t:sections.main_product.blocks.title.settings.show_stock.label",
          "default": true
        },
        {
          "type": "checkbox",
          "id": "show_sku",
          "label": "t:sections.main_product.blocks.title.settings.show_sku.label",
          "default": true
        },
        {
          "type": "checkbox",
          "id": "show_barcode",
          "label": "t:sections.main_product.blocks.title.settings.show_barcode.label",
          "default": false
        },
        {
          "type": "text",
          "id": "barcode_label",
          "label": "t:sections.main_product.blocks.title.settings.barcode_label.label",
          "info": "t:sections.main_product.blocks.title.settings.barcode_label.info",
          "default": "Barcode",
          "visible_if": "{{ block.settings.show_barcode }}"
        },
        {
          "type": "checkbox",
          "id": "show_rating",
          "default": false,
