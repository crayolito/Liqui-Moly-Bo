<script>
  document.addEventListener('DOMContentLoaded', function() {
    const SELECTOR = '.shopify-payment-button__button';
    const BIND_DELAYS = [0, 500, 1000, 2000];

    function validarVariante(variant, quantity) {
      if (!variant.available) {
        window.CustomCheckout.debug.error(' Variante no disponible');
        return { valida: false, mensaje: 'Lo sentimos, esta variante no est disponible.' };
      }

      if (variant.inventory_policy !== 'continue' && variant.inventory_quantity <= 0) {
        window.CustomCheckout.debug.error(' Sin inventario');
        return { valida: false, mensaje: 'Lo sentimos, este producto est agotado.' };
      }

      if (quantity <= 0) {
        return { valida: false, mensaje: ' La cantidad debe ser mayor a 0.' };
      }

      if (variant.inventory_policy !== 'continue' && quantity > variant.inventory_quantity) {
        return { 
          valida: false, 
          mensaje: `Solo tenemos ${variant.inventory_quantity} unidades disponibles.` 
        };
      }

      return { valida: true };
    }

    function obtenerDatosProducto(button) {
      try {
        const productForm = button.closest('product-form');
        if (!productForm) return null;

        const form = productForm.querySelector('form');
        if (!form) return null;

        const productJson = document.querySelector('[data-product-json]');
        if (!productJson) return null;

        const product = JSON.parse(productJson.textContent);
        const formData = new FormData(form);
        const selectedVariantId = formData.get('id');
        const selectedVariant = product.variants.find(v => v.id == selectedVariantId);

        if (!selectedVariant) return null;

        return {
          product,
          variant: selectedVariant,
          quantity: parseInt(formData.get('quantity')) || 1,
          formData
        };
      } catch (error) {
        window.CustomCheckout.debug.error('Error obteniendo datos del producto:', error);
        return null;
      }
    }

    function bindBuyNowButtons() {
      const buttons = document.querySelectorAll(SELECTOR);
      
      buttons.forEach(button => {
        if (button.hasAttribute('data-custom-bound')) return;
        
        button.setAttribute('data-custom-bound', 'true');

        button.addEventListener('click', function(e) {
          if (!window.CustomCheckout.config.USE_CUSTOM_CHECKOUT) {
            return; // Comportamiento normal
          }

          e.preventDefault();
          e.stopPropagation();

          const datos = obtenerDatosProducto(button);
          if (!datos) {
            window.CustomCheckout.debug.warn(' Fallback a checkout normal');
            window.location.href = button.href || '#';
            return;
          }

          const { product, variant, quantity } = datos;

          // Validar disponibilidad
          const validacion = validarVariante(variant, quantity);
          if (!validacion.valida) {
            alert(validacion.mensaje);
            return;
          }

          // Construir item del pedido
          const orderData = window.CustomCheckout.construirDatosPedido({
            variant_id: variant.id,
            product_id: product.id,
            title: product.title,
            variant_title: variant.title || 'Default Title',
            quantity: quantity,
            price: variant.price / 100,
            price_cents: variant.price,
            compare_at_price: variant.compare_at_price ? (variant.compare_at_price / 100) : 0,
            sku: variant.sku || '',
            image: variant.featured_image?.url || product.featured_image || '',
            url: product.url,
            vendor: product.vendor || '',
            product_type: product.type || '',
            tags: product.tags || [],
            options: variant.options || [],
            option1: variant.option1 || null,
            option2: variant.option2 || null,
            option3: variant.option3 || null,
          });

          // Procesar checkout (sin limpiar carrito porque es compra directa)
          window.CustomCheckout.procesar(orderData, {
            limpiarCarrito: false,
            cerrarDrawer: false
          });
        });
      });
    }

    // Inicializacin con delays
    BIND_DELAYS.forEach(delay => {
      if (delay === 0) {
        bindBuyNowButtons();
      } else {
        setTimeout(bindBuyNowButtons, delay);
      }
    });

    // Observer optimizado
    const observer = new MutationObserver(function(mutations) {
      let shouldRebind = false;
      
      for (const mutation of mutations) {
        if (mutation.type === 'childList' && mutation.addedNodes.length > 0) {
          for (const node of mutation.addedNodes) {
            if (node.nodeType === 1 && (
              node.matches?.(SELECTOR) || 
              node.querySelector?.(SELECTOR)
            )) {
              shouldRebind = true;
              break;
            }
          }
          if (shouldRebind) break;
        }
      }
      
      if (shouldRebind) {
        setTimeout(bindBuyNowButtons, 100);
      }
    });

    observer.observe(document.body, {
      childList: true,
      subtree: true
    });
  });
</script>