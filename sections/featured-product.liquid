{% comment %}theme-check-disable TemplateLength{% endcomment %}
{% liquid
  assign hide_because_empty = false
  if request.design_mode == false and section.settings.hide_if_empty and section.settings.product == blank
    assign hide_because_empty = true
  endif
%}
{%- unless hide_because_empty -%}
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-pills.css' | asset_url | stylesheet_tag }}
  {{ 'component-tooltip.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>

  {% liquid
    assign product = section.settings.product
    assign first_3d_model = product.media | where: 'media_type', 'model' | first
    assign product_form_id = 'product-form-' | append: section.id
    assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src'

    assign color_scheme_string = section.settings.color_scheme | append: ''
    assign block_section_class = 'block-section'
    unless color_scheme_string == 'background-1'
      assign block_section_class = block_section_class | append: ' block-section-padded'
    endunless

    assign preorderSuffixArray = product.template_suffix | split: 'pre-order-'
    assign is_pre_order = false
    if product.template_suffix != blank and preorderSuffixArray[0] == blank or product.template_suffix == 'pre-order'
      assign is_pre_order = true
    endif
  %}

  {%- liquid
    assign has_media = false
    if product.media.size > 0 or settings.product_placeholder_image
      assign has_media = true
    endif

    # We set the media to true a product is not selected to maintain the preview.
    if product == blank
      assign has_media = true
    endif
  -%}
  {% if has_media %}
    {{ 'flickity.css' | asset_url | stylesheet_tag }}
  {% endif %}

  {%- if first_3d_model -%}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  <style>
    #shopify-section-{{ section.id }} .block-section {
    	margin-top: {{ section.settings.spacing_top | times: 0.8 | round: 0 }}px;
    	margin-bottom: {{ section.settings.spacing_bottom | times: 0.8 | round: 0 }}px;
    }

    #shopify-section-{{ section.id }} .block-section-padded {
    	margin-top: 0;
    	margin-bottom: 0;
    	padding-top: {{ section.settings.spacing_top | times: 0.8 | round: 0 }}px;
    	padding-bottom: {{ section.settings.spacing_bottom | times: 0.8 | round: 0 }}px;
    }

    @media (min-width: 990px) {
    	#shopify-section-{{ section.id }} .block-section {
    		margin-top: {{ section.settings.spacing_top }}px;
    		margin-bottom: {{ section.settings.spacing_bottom }}px;
    	}

    	#shopify-section-{{ section.id }} .block-section-padded {
    		margin-top: 0;
    		margin-bottom: 0;
    		padding-top: {{ section.settings.spacing_top }}px;
    		padding-bottom: {{ section.settings.spacing_bottom }}px;
    	}
    }
  </style>

  {%- capture visibility_classes -%}{%- render 'section-visibility-classes' -%}{%- endcapture -%}
  {%- assign visibility_classes = visibility_classes | strip -%}

  <div class="{{ visibility_classes }}">
    <div class="color-{{ section.settings.color_scheme }}">
      <div class="container {% if has_media == false %}container-narrow{% endif %}">
        <div class="{{ block_section_class }}">
          {% if section.settings.heading != blank or section.settings.subheading != blank %}
            <div class="block-block-section-title-wrapper">
              <div class="block-section-title-wrap">
                {% if section.settings.heading != blank %}
                  <{{ section.settings.heading_tag }} class="block-section-title heading-size-{{ section.settings.heading_size }}">
                    {{ section.settings.heading }}
                  </{{ section.settings.heading_tag }}>
                {% endif %}
                {% if section.settings.subheading != blank %}
                  <p class="block-section-subtitle">{{ section.settings.subheading }}</p>
                {% endif %}
              </div>
            </div>
          {% endif %}

          <div
            id="ProductPage-{{ section.id }}"
            data-section-id="{{ section.id }}"
            class="product product-page product-section-featured {% if has_media == false %}product-no-media{% endif %}"
          >
            {% if has_media or product == blank %}
              <div class="product-media-gallery-wrap">
                <media-gallery
                  id="MediaGallery-{{ section.id }}"
                  role="region"
                  class="product-media-gallery"
                  aria-label="{{ 'products.product.media.gallery_viewer' | t }}"
                  hide-variants="{{ section.settings.hide_variants }}"
                >
                  <div class="product-media-wrapper">
                    {% if product.media %}
                      {%- liquid
                        assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media | default: settings.product_placeholder_image
                        assign ratio = 0
                        if section.settings.image_ratio == 'square'
                          assign ratio = 1
                        elsif section.settings.image_ratio == 'portrait'
                          assign ratio = 0.75
                        elsif section.settings.image_ratio == 'landscape'
                          assign ratio = 1.777
                        endif
                      -%}

                      <div class="product-media-main-wrapper color-background-1">
                        {%- assign media_position = 1 -%}
                        <div
                          class="product-media-main product-media is-active"
                          data-media-id="{{ featured_media.id }}"
                        >
                          {% render 'product-gallery-media',
                            media: featured_media,
                            position: media_position,
                            media_width: 800,
                            lazyload: true,
                            modal_id: section.id,
                            image_zoom_type: section.settings.image_zoom_type,
                            custom_ratio: ratio
                          %}
                        </div>

                        {%- for media in product.media -%}
                          {%- if media.id != featured_media.id -%}
                            {%- liquid
                              assign media_position = media_position | default: 1 | plus: 1
                              assign variant_image = false
                              if section.settings.hide_variants and variant_images contains media.src
                                assign variant_image = true
                              endif
                            -%}
                            {% unless variant_image %}
                              <div
                                class="product-media-main product-media"
                                data-media-id="{{ media.id }}"
                              >
                                {% render 'product-gallery-media',
                                  media: media,
                                  position: media_position,
                                  media_width: 800,
                                  lazyload: true,
                                  modal_id: section.id,
                                  image_zoom_type: section.settings.image_zoom_type,
                                  custom_ratio: ratio
                                %}
                              </div>
                            {% endunless %}
                          {%- endif -%}
                        {%- endfor -%}

                        {% if section.settings.arrow_navigation and product.media.size > 1 %}
                          <carousel-navigation
                            for="MediaGallery-{{ section.id }}"
                            data-wrap="true"
                          >
                            <div class="pill-nav pill-nav-slider pill-nav-slider-inline-nav">
                              <button class="pill-nav-item pill-nav-item-prev js-prev">
                                <span class="visually-hidden">
                                  {{- 'products.product.media.previous_image' | t -}}
                                </span>
                                {% render 'angle', direction: 'left' %}
                              </button>
                              <button class="pill-nav-item pill-nav-item-next js-next">
                                <span class="visually-hidden">
                                  {{- 'products.product.media.next_image' | t -}}
                                </span>
                                {% render 'angle', direction: 'right' %}
                              </button>
                            </div>
                          </carousel-navigation>
                        {% endif %}
                      </div>

                      {%- assign media_position = 1 -%}
                      <div class="product-media-scroll-wrap">
                        <div class="product-media-scroller">
                          <div class="product-media-thumbnails">
                            <div
                              class="
                                product-media-thumbnail product-media is-active color-background-1
                                {% if section.settings.hide_variants and variant_images contains featured_media.src %} product-media-thumbnail-variant{% endif %}
                              "
                              data-media-id="{{ featured_media.id }}"
                            >
                              {% render 'product-gallery-media-thumbnail',
                                media: featured_media,
                                position: media_position,
                                media_width: 400,
                                lazyload: true
                              %}
                            </div>

                            {% for media in product.media %}
                              {%- if media.id != featured_media.id -%}
                                {%- assign media_position = media_position
                                  | default: 1
                                  | plus: 1
                                -%}
                                <div
                                  class="
                                    product-media-thumbnail product-media color-background-1
                                    {% if section.settings.hide_variants and variant_images contains media.src %} product-media-thumbnail-variant{% endif %}
                                  "
                                  data-media-id="{{ media.id }}"
                                >
                                  {% render 'product-gallery-media-thumbnail',
                                    media: media,
                                    position: media_position,
                                    media_width: 400,
                                    lazyload: true
                                  %}
                                </div>
                              {% endif %}
                            {% endfor %}
                          </div>
                        </div>

                        {% if section.settings.thumbnail_navigation %}
                          <product-thumbnail-navigation
                            for="MediaGallery-{{ section.id }}"
                            class="hidden"
                          >
                            <div class="pill-nav pill-nav-slider media-pill-nav-slider">
                              <button
                                class="pill-nav-item pill-nav-item-prev pill-nav-item-disabled focus-none js-prev"
                                aria-disabled="true"
                              >
                                <span class="visually-hidden">
                                  {{- 'sections.slideshow.previous_slide' | t -}}
                                </span>
                                {% render 'angle', direction: 'up' %}
                              </button>
                              <button
                                class="pill-nav-item pill-nav-item-next js-next"
                                aria-disabled="false"
                              >
                                <span class="visually-hidden">
                                  {{- 'sections.slideshow.next_slide' | t -}}
                                </span>
                                {% render 'angle', direction: 'down' %}
                              </button>
                            </div>
                          </product-thumbnail-navigation>
                        {% endif %}
                      </div>
                    {% else %}
                      <div class="product-media-main-wrapper color-background-1">
                        <div class="product-media-main product-media is-active">
                          {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                        </div>
                      </div>
                      <div class="product-media-scroll-wrap">
                        <div class="product-media-scroller">
                          <div class="product-media-thumbnails">
                            <div class="product-media-thumbnail product-media color-background-1 is-active">
                              {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                            </div>
                          </div>
                        </div>
                      </div>
                    {% endif %}
                  </div>
                </media-gallery>

                {% if section.settings.image_zoom_type != 'none' %}
                  <product-modal
                    id="ProductModal-{{ section.id }}"
                    class="product-media-modal media-modal"
                  >
                    <div
                      class="product-media-modal-dialog"
                      role="dialog"
                      aria-modal="true"
                      aria-label="{{ 'products.product.media.gallery' | t }}"
                      tabindex="-1"
                    >
                      <button
                        id="ModalClose-{{ section.id }}"
                        type="button"
                        class="product-media-modal-toggle"
                        aria-label="{{ 'accessibility.close' | t }}"
                      >
                        {% render 'icons', icon: 'cross' %}
                      </button>
                      <div
                        class="product-media-modal-content"
                        role="document"
                        aria-label="{{ 'products.product.media.gallery' | t }}"
                        tabindex="0"
                      >
                        {%- liquid
                          assign featured_media = product.selected_or_first_available_variant.featured_media | default: settings.product_placeholder_image
                          if featured_media
                            render 'product-gallery-media-modal', media: featured_media, variant_image: section.settings.hide_variants
                          endif
                        -%}

                        {%- for media in product.media -%}
                          {%- liquid
                            assign variant_image = false
                            if section.settings.hide_variants and variant_images contains media.src
                              assign variant_image = true
                            endif

                            unless media.id == featured_media.id or variant_image
                              render 'product-gallery-media-modal', media: media, variant_image: variant_image
                            endunless
                          -%}
                        {%- endfor -%}
                      </div>
                    </div>
                  </product-modal>
                {% endif %}

                {% if section.settings.image_disclaimer != blank %}
                  <div class="product-media-gallery-message">
                    <div class="rte">
                      {{ section.settings.image_disclaimer }}
                    </div>
                  </div>
                {% endif %}
              </div>
            {% endif %}

            <div class="product-info-wrap-content">
              <div id="Product-{{ section.id }}" class="product-info">
                {% for block in section.blocks %}
                  {% case block.type %}
                    {% when '@app' %}
                      <div class="product-info-secondary" {{ block.shopify_attributes }}>
                        {% render block %}
                      </div>

                    {%- when 'text' -%}
                      {% if block.settings.text != blank
                        or block.settings.text_2 != blank
                        or block.settings.text_3 != blank
                        or block.settings.text_4 != blank
                      %}
                        {% assign color_scheme_string = block.settings.color_scheme | append: '' %}
                        <div
                          class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                          {{ block.shopify_attributes }}
                        >
                          {% if color_scheme_string != 'background-1' %}
                            <div
                              class="product-info-secondary card color-{{ block.settings.color_scheme }}"
                            >
                          {% else %}
                            <div class="product-info-secondary">
                          {% endif %}
                          {% if block.settings.text != blank %}
                            <div class="product-info-secondary-line rte">
                              {% if block.settings.icon != blank %}
                                {% render 'icons', icon: block.settings.icon %}
                              {% endif %}
                              <span>{{ block.settings.text }}</span>
                            </div>
                          {% endif %}
                          {% if block.settings.text_2 != blank %}
                            <div class="product-info-secondary-line rte">
                              {% if block.settings.icon_2 != blank %}
                                {% render 'icons', icon: block.settings.icon_2 %}
                              {% endif %}
                              <span>{{ block.settings.text_2 }}</span>
                            </div>
                          {% endif %}
                          {% if block.settings.text_3 != blank %}
                            <div class="product-info-secondary-line rte">
                              {% if block.settings.icon_3 != blank %}
                                {% render 'icons', icon: block.settings.icon_3 %}
                              {% endif %}
                              <span>{{ block.settings.text_3 }}</span>
                            </div>
                          {% endif %}
                          {% if block.settings.text_4 != blank %}
                            <div class="product-info-secondary-line rte">
                              {% if block.settings.icon_4 != blank %}
                                {% render 'icons', icon: block.settings.icon_4 %}
                              {% endif %}
                              <span>{{ block.settings.text_4 }}</span>
                            </div>
                          {% endif %}
                        </div>
                        </div>
                      {% endif %}

                    {% when 'title' %}
                      <div
                        class="product-info-main product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        {% if block.settings.show_custom_badges %}
                          {% render 'card-product-custom-badges',
                            product: product,
                            class: 'product-info-badges'
                          %}
                        {% endif %}

                        {% if block.settings.show_vendor and product.vendor != blank %}
                          {% liquid
                            assign matching_collection = false
                            assign vendor_handle = product.vendor | handleize
                            for collection in product.collections
                              if collection.handle == vendor_handle
                                assign matching_collection = true
                              endif
                            endfor
                          %}
                          <div class="product-info-eyebrow">
                            {% if matching_collection %}
                              <a href="{{ routes.collections_url }}/{{ vendor_handle }}">
                                {{- product.vendor -}}
                              </a>
                            {% else %}
                              {{ product.vendor | link_to_vendor }}
                            {% endif %}
                          </div>
                        {% endif %}

                        <{{ block.settings.block_heading_tag }} class="product-info-heading h2">
                          {% if product.title != blank %}
                            {{ product.title | escape }}
                          {% else %}
                            {{ 'onboarding.product_title' | t }}
                          {% endif %}
                        </{{ block.settings.block_heading_tag }}>

                        {% liquid
                          assign product_info_meta_stretch_class = ''
                          if product.selected_or_first_available_variant.barcode and block.settings.show_barcode and product.selected_or_first_available_variant.sku and block.settings.show_sku
                            assign product_info_meta_stretch_class = 'product-info-meta-stretch'
                          endif
                        %}

                        {% if block.settings.show_stock
                          or block.settings.show_sku
                          or block.settings.show_barcode
                          or block.settings.show_rating
                        %}
                          <div
                            id="ProductInfoMeta-{{ section.id }}"
                            class="product-info-meta product-info-meta-{{ section.id }} {{ product_info_meta_stretch_class }} {% if product.selected_or_first_available_variant == nil %}visually-hidden{% endif %}"
                          >
                            {% unless settings.catalog_enabled %}
                              {% if block.settings.show_stock %}
                                {% if product != blank %}
                                  <div
                                    id="ProductAvailability-{{ section.id }}"
                                    class="product-info-meta-item"
                                  >
                                    {% render 'product-availability', product: product %}
                                  </div>
                                {% else %}
                                  <div
                                    id="ProductAvailability-{{ section.id }}"
                                    class="product-info-meta-item"
                                  >
                                    <div class="badge-stock badge-stock-out">
                                      <span class="badge-stock-dot"></span>
                                      <span class="js-stock-text">
                                        {{- 'products.product.availability.out_of_stock' | t -}}
                                      </span>
                                    </div>
                                  </div>
                                {% endif %}
                              {% endif %}
                            {% endunless %}

                            {% if block.settings.show_sku %}
                              {% if product.selected_or_first_available_variant.sku != blank %}
                                <span class="product-info-meta-item product-info-meta-item-code">
                                  {{ 'products.product.sku' | t }}:
                                  <span
                                    id="ProductSku-{{ section.id }}"
                                    class="ProductSku-{{ section.id }}"
                                  >
                                    {{- product.selected_or_first_available_variant.sku -}}
                                  </span>
                                </span>
                              {% endif %}
                            {% endif %}

                            {% if block.settings.show_barcode %}
                              {% if product.selected_or_first_available_variant.barcode != blank %}
                                <span class="product-info-meta-item product-info-meta-item-code">
                                  {% if block.settings.barcode_label != blank %}
                                    {{ block.settings.barcode_label }}:
                                  {% endif %}
                                  <span
                                    id="ProductBarcode-{{ section.id }}"
                                    class="ProductBarcode-{{ section.id }}"
                                  >
                                    {{- product.selected_or_first_available_variant.barcode -}}
                                  </span>
                                </span>
                              {% endif %}
                            {% endif %}

                            {% if block.settings.show_rating %}
                              <div class="product-info-meta-item last {% if block.settings.hide_empty_star_rating %}product-rating-hide-empty{% endif %}">
                                {% render 'product-rating-widget', product: product %}
                              </div>
                            {% endif %}
                          </div>
                        {% endif %}
                      </div>
                    {% when 'extra' %}
                      <div
                        class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        <div class="product-info-extra">
                          {% if block.settings.text != blank %}
                            <div class="product-info-extra-aux rte">
                              {{ block.settings.text }}
                            </div>
                          {% endif %}

                          {% if block.settings.show_facebook
                            or block.settings.show_twitter
                            or block.settings.show_linkedin
                          %}
                            <div class="product-info-extra-sharing">
                              <ul class="list-social">
                                {% if block.settings.show_facebook %}
                                  <li class="list-social-item">
                                    <a
                                      href="https://www.facebook.com/sharer.php?u={{ shop.url }}{{ product.url | url_encode }}"
                                      data-share-url="https://www.facebook.com/sharer.php?u="
                                      target="_blank"
                                      class="list-social-link"
                                    >
                                      <span class="visually-hidden">
                                        {{- 'general.social.share_on_facebook' | t -}}
                                      </span>
                                      {% render 'icons', icon: 'facebook' %}
                                    </a>
                                  </li>
                                {% endif %}
                                {% if block.settings.show_twitter %}
                                  <li class="list-social-item">
                                    <a
                                      href="https://x.com/share?url={{ shop.url }}{{ product.url | url_encode }}"
                                      data-share-url="https://x.com/share?url="
                                      target="_blank"
                                      class="list-social-link"
                                    >
                                      <span class="visually-hidden">
                                        {{- 'general.social.share_on_x' | t -}}
                                      </span>
                                      {% render 'icons', icon: 'x' %}
                                    </a>
                                  </li>
                                {% endif %}
                                {% if block.settings.show_linkedin %}
                                  <li class="list-social-item">
                                    <a
                                      href="https://www.linkedin.com/sharing/share-offsite/?url={{ shop.url }}{{ product.url | url_encode }}"
                                      data-share-url="https://www.linkedin.com/sharing/share-offsite/?url="
                                      target="_blank"
                                      class="list-social-link"
                                    >
                                      <span class="visually-hidden">
                                        {{- 'general.social.share_on_linkedin' | t -}}
                                      </span>
                                      {% render 'icons', icon: 'linkedin' %}
                                    </a>
                                  </li>
                                {% endif %}
                              </ul>
                            </div>
                          {% endif %}
                        </div>
                      </div>

                    {%- when 'price' -%}
                      {% unless settings.catalog_enabled %}
                        <div
                          class="product-block product-block-price {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                          {{ block.shopify_attributes }}
                        >
                          {%- capture login_for_price -%}{%- render 'price-get-login-for-price', product: product -%}{%- endcapture -%}
                          {%- assign login_for_price = login_for_price | strip -%}
                          {% if login_for_price == 'true' %}
                            <div
                              class="{% if product.selected_or_first_available_variant == nil %}visually-hidden{% endif %}"
                              id="price-{{ section.id }}"
                              role="status"
                            >
                              {% render 'price-login-for-price' %}
                            </div>
                          {% else %}
                            <div
                              class="{% if product.selected_or_first_available_variant == nil %}visually-hidden{% endif %}"
                              id="price-{{ section.id }}"
                              role="status"
                            >
                              {% render 'price',
                                product: product,
                                discount_badge: true,
                                use_variant: true
                              %}
                            </div>

                            {% if product != blank %}
                              {%- form 'product',
                                product,
                                id: 'product-form-installment',
                                class: 'installment',
                                data-event-context: 'FeaturedProductSectionInstallment'
                              -%}
                                <input
                                  type="hidden"
                                  name="id"
                                  value="{{ product.selected_or_first_available_variant.id }}"
                                >
                                {{ form | payment_terms }}
                              {%- endform -%}
                            {% endif %}
                          {% endif %}
                        </div>
                      {% endunless %}

                    {%- when 'variants' -%}
                      <div class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}">
                        {% render 'product-variant-picker',
                          section: section,
                          block: block,
                          product: product,
                          product_form_id: product_form_id,
                          update_url: false
                        %}
                      </div>

                    {% when 'buy_buttons' %}
                      {% unless settings.catalog_enabled %}
                        {%- capture login_for_price -%}{%- render 'price-get-login-for-price', product: product -%}{%- endcapture -%}
                        {%- assign login_for_price = login_for_price | strip -%}
                        {% if login_for_price != 'true' %}
                          {%- liquid
                            assign gift_card_recipient_feature_active = false
                            if block.settings.show_gift_card_recipient and product.gift_card?
                              assign gift_card_recipient_feature_active = true
                            endif

                            assign show_dynamic_checkout = false
                            if block.settings.show_buy_now and gift_card_recipient_feature_active == false and is_pre_order == false
                              assign show_dynamic_checkout = true
                            endif
                          -%}

                          <div
                            class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                            {{ block.shopify_attributes }}
                          >
                            {% if product != blank %}
                              <product-form
                                class="product-form"
                                data-hide-errors="{{ gift_card_recipient_feature_active }}"
                              >
                                <div
                                  class="product-form-error-message-wrapper form-status form-status-error"
                                  role="alert"
                                  hidden
                                >
                                  <span class="product-form-error-message"></span>
                                </div>

                                {% form 'product',
                                  product,
                                  id: product_form_id,
                                  class: 'form',
                                  novalidate: 'novalidate',
                                  data-type: 'add-to-cart-form',
                                  data-stock-threshold: settings.only_x_left,
                                  data-event-context: 'FeaturedProductSection'
                                %}
                                  <input
                                    type="hidden"
                                    name="id"
                                    value="{{ product.selected_or_first_available_variant.id }}"
                                    {% if product.selected_or_first_available_variant.available
                                        == false
                                      or product.selected_or_first_available_variant == null
                                    %}
                                      disabled
                                    {% endif %}
                                  >

                                  {%- if gift_card_recipient_feature_active -%}
                                    {%- render 'gift-card-recipient-form',
                                      product: product,
                                      form: form,
                                      section: section
                                    -%}
                                  {%- endif -%}

                                  <div class="product-actions">
                                    <div class="product-actions-add-to-cart">
                                      {% if block.settings.show_quantity %}
                                        <quantity-input class="quantity-input">
                                          <button
                                            type="button"
                                            name="minus"
                                            class="quantity-input-button quantity-input-minus"
                                          >
                                            &minus;
                                            <span class="visually-hidden">
                                              {{-
                                                'products.product.quantity.decrease'
                                                | t: product: product.title
                                                | escape
                                              -}}
                                            </span>
                                          </button>
                                          <label
                                            for="quantity-input-{{ section.id }}"
                                            class="visually-hidden"
                                          >
                                            {{- 'products.product.quantity.label' | t -}}
                                          </label>
                                          <input
                                            name="quantity"
                                            type="number"
                                            class="quantity-input-field"
                                            id="quantity-input-{{ section.id }}"
                                            min="1"
                                            value="1"
                                            form="{{ product_form_id }}"
                                          >
                                          <button
                                            type="button"
                                            name="plus"
                                            class="quantity-input-button quantity-input-plus"
                                          >
                                            &plus;
                                            <span class="visually-hidden">
                                              {{-
                                                'products.product.quantity.increase'
                                                | t: product: product.title
                                                | escape
                                              -}}
                                            </span>
                                          </button>
                                        </quantity-input>
                                      {% endif %}

                                      {% if block.settings.show_add_to_cart %}
                                        <button
                                          type="submit"
                                          name="add"
                                          class="button button-add-to-cart {% if show_dynamic_checkout %} button-secondary{% endif %}"
                                          {% if product.selected_or_first_available_variant.available
                                              == false
                                            or product.selected_or_first_available_variant == null
                                          %}
                                            disabled
                                          {% endif %}
                                        >
                                          {%- if product.selected_or_first_available_variant == null
                                          -%}
                                            <span>
                                              {{-
                                                'products.product.availability.unavailable'
                                                | t
                                              -}}
                                            </span>
                                          {%- elsif product.selected_or_first_available_variant.available
                                            and is_pre_order
                                          -%}
                                            <span>{{ 'products.product.pre_order' | t }}</span>
                                          {%- elsif product.selected_or_first_available_variant.inventory_policy
                                              == 'continue'
                                            and product.selected_or_first_available_variant.inventory_quantity
                                              <= 0
                                          -%}
                                            <span>{{ 'products.product.backorder' | t }}</span>
                                          {%- elsif product.selected_or_first_available_variant.available -%}
                                            <span>{{ 'products.product.add_to_cart' | t }}</span>
                                          {%- else -%}
                                            <span>
                                              {{-
                                                'products.product.availability.out_of_stock'
                                                | t
                                              -}}
                                            </span>
                                          {%- endif -%}

                                          <div class="button-overlay-spinner hidden">
                                            <span class="spinner-xs"></span>
                                          </div>
                                        </button>
                                      {% endif %}
                                    </div>

                                    {% if show_dynamic_checkout %}
                                      <div class="product-actions-buy-now">
                                        {{ form | payment_button }}
                                      </div>
                                    {% endif %}
                                  </div>
                                {% endform %}
                              </product-form>
                            {% else %}
                              <div class="product-form" {{ block.shopify_attributes }}>
                                <div class="product-actions">
                                  <div class="product-actions-add-to-cart">
                                    {% if block.settings.show_quantity %}
                                      <quantity-input class="quantity-input">
                                        <button
                                          type="button"
                                          name="minus"
                                          class="quantity-input-button quantity-input-minus"
                                        >
                                          &minus;
                                          <span class="visually-hidden">
                                            {{-
                                              'products.product.quantity.decrease'
                                              | t: product: product.title
                                              | escape
                                            -}}
                                          </span>
                                        </button>
                                        <label for="quantity-input" class="visually-hidden">
                                          {{- 'products.product.quantity.label' | t -}}
                                        </label>
                                        <input
                                          name="quantity"
                                          type="number"
                                          class="quantity-input-field"
                                          id="quantity-input"
                                          min="1"
                                          value="1"
                                          form="{{ product_form_id }}"
                                        >
                                        <button
                                          type="button"
                                          name="plus"
                                          class="quantity-input-button quantity-input-plus"
                                        >
                                          &plus;
                                          <span class="visually-hidden">
                                            {{-
                                              'products.product.quantity.increase'
                                              | t: product: product.title
                                              | escape
                                            -}}
                                          </span>
                                        </button>
                                      </quantity-input>
                                    {% endif %}

                                    {% if block.settings.show_add_to_cart %}
                                      <button
                                        type="submit"
                                        name="add"
                                        class="button button-secondary button-add-to-cart"
                                        disabled=""
                                      >
                                        <span>
                                          {{- 'products.product.availability.out_of_stock' | t -}}
                                        </span>
                                        <span class="button-overlay-spinner hidden">
                                          <span class="spinner-xs"></span>
                                        </span>
                                      </button>
                                    {% endif %}
                                  </div>
                                </div>
                              </div>
                            {% endif %}
                          </div>
                        {% endif %}
                      {% endunless %}

                    {% when 'request_information' %}
                      <div
                        class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        {% render 'main-product-request-information',
                          product: product,
                          settings: block.settings,
                          block: block
                        %}
                      </div>

                    {% when 'contact_details' %}
                      <div
                        class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        {% render 'main-product-contact-details',
                          settings: block.settings,
                          block: block
                        %}
                      </div>

                    {% when 'product_signup' %}
                      {% liquid
                        assign show_product_signup = true
                        if product.selected_or_first_available_variant.available and block.settings.show_when == 'out_of_stock'
                          assign show_product_signup = false
                        endif
                      %}
                      <div
                        id="ProductSignup-{{ section.id }}"
                        class="
                          product-block
                          {% if block.settings.show_bottom_divider %}has-divider{% endif %}
                          {% if show_product_signup == false %}hidden{% endif %}
                        "
                        {{ block.shopify_attributes }}
                      >
                        {% assign form_id = 'form-'
                          | append: section.id
                          | append: '-block-'
                          | append: forloop.index
                        %}
                        {% render 'main-product-signup',
                          id: form_id,
                          product: product,
                          block: block
                        %}
                      </div>

                    {% when 'stock_bar' %}
                      <div
                        class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        {% render 'main-product-stock-bar',
                          product: product,
                          block_settings: block.settings,
                          block: block
                        %}
                      </div>

                    {% when 'info_box' %}
                      <div
                        class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        {% render 'main-product-info-box', settings: block.settings, block: block %}
                      </div>

                    {% when 'image' %}
                      <div
                        class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        {% render 'main-product-image-block',
                          settings: block.settings,
                          block: block
                        %}
                      </div>

                    {% when 'product_metadata' %}
                      <div
                        class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        {% render 'main-product-metadata-block',
                          product: product,
                          settings: block.settings,
                          block: block
                        %}
                      </div>

                    {%- when 'details' -%}
                      <div class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}">
                        {% render 'main-product-details',
                          product: product,
                          block: block,
                          position: 'right'
                        %}
                      </div>

                    {%- when 'range_indicator' -%}
                      {% if block.settings.value != blank or request.design_mode %}
                        <div
                          class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                          {{ block.shopify_attributes }}
                        >
                          {% render 'main-product-range-indicator',
                            title: block.settings.label,
                            from: block.settings.range_from,
                            to: block.settings.range_to,
                            value: block.settings.value,
                            track_color: block.settings.track_color,
                            filled_color: block.settings.filled_color,
                            block: block
                          %}
                        </div>
                      {% endif %}

                    {%- when 'custom_liquid' -%}
                      {% capture liquid_output %}{{ block.settings.custom_liquid }}{% endcapture %}
                      {% if liquid_output != blank %}
                        <div class="custom-liquid-right" {{ block.shopify_attributes }}>
                          <div class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}">
                            {% if block.settings.title != blank or block.settings.icon != blank %}
                              <p class="custom-liquid-right-title h5">
                                {% if block.settings.icon != blank %}
                                  {% render 'icons', icon: block.settings.icon %}
                                {% endif %}
                                {{ block.settings.title }}
                              </p>
                            {% endif %}

                            {{ block.settings.custom_liquid }}
                          </div>
                        </div>
                      {% endif %}

                    {%- when 'personalization_text' -%}
                      <div
                        class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                        {{ block.shopify_attributes }}
                      >
                        <div
                          class="product-block {% if block.settings.show_bottom_divider %}has-divider{% endif %}"
                          {{ block.shopify_attributes }}
                        >
                          {% render 'main-product-personalization-fields',
                            product: product,
                            settings: block.settings,
                            block: block,
                            product_form_id: product_form_id
                          %}
                        </div>
                    {%- when 'countdown' -%}
                      {% render 'main-product-countdown', block: block %}
                  {% endcase %}
                {% endfor %}

                <div class="product-info-secondary">
                  <a
                    {% if product == blank %}
                      role="link" aria-disabled="true"
                    {% else %}
                      href="{{ product.url }}"
                    {% endif %}
                    class="button-text-link"
                  >
                    {{ 'products.product.view_full_details' | t }}
                    {% render 'angle', direction: 'right' %}
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  {%- if first_3d_model -%}
    <script type="application/json" id="ProductJSON-{{ product.id }}">
      {{ product.media | where: 'media_type', 'model' | json }}
    </script>

    <script src="{{ 'product-model.js' | asset_url }}" defer></script>
  {%- endif -%}

  {% if has_media %}
    <script src="{{ 'flickity.pkgd.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
    {% if section.settings.arrow_navigation %}
      <script src="{{ 'carousel-slider.js' | asset_url }}" defer="defer"></script>
    {% endif %}
  {% endif %}

  <script type="application/ld+json">
    {{ product | structured_data }}
  </script>
{%- endunless -%}
{% schema %}
